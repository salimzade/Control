      }
    },
    {
      "name": "Markup text",
      "scope": ["text.html", "keyword.operator.assignment"],
      "settings": {
        "foreground": "#ffffffee"
      }
    },
    {
      "name": "Markup quote",
      "scope": "markup.quote",
