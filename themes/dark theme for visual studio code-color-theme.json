      "settings": {
        "foreground": "#72f1b8"
      }
    },
    {
      "name": "JS Switch control",
      "scope": "switch-block.expr.js",
      "settings": {
        "foreground": "#72f1b8"
      }
    },
    {
      "name": "JS object path",
